[calendars]

[[fam_local]]
path = ~/calendars/fam/*
type = discover

[[infocus_local]]
path = ~/calendars/infocus/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = john@infocusdata.com
